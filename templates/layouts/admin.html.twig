{% extends 'layouts/default.html.twig' %}

{% block title %}
    {{ admin_title is defined and admin_title ? admin_title ~ ' Â· ' : '' }}{{ site_title is defined and site_title ? site_title : 'aavion Studio'|trans }}
{% endblock %}

{% block header %}
    {% set header_title = admin_header_title|default('Admin'|trans) %}
    {% set header_image = admin_header_image|default(header_image|default(null)) %}
    {{ parent() }}
{% endblock %}

{% block body %}
    {% set body_class = (body_class is defined and body_class ? body_class ~ ' ' : '') ~ 'admin' %}
    {{ parent() }}
{% endblock %}

{% block content %}{% endblock %}
